{
  "name": "hackatime",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "versionCode": 1,
  "scripts": {
    "start": "expo start",
    "start:dev": "APP_VARIANT=development EDGE_PATH=/etc/profiles/per-user/stef/bin/chromium expo start --dev-client",
    "start:dev:clear": "APP_VARIANT=development EDGE_PATH=/etc/profiles/per-user/stef/bin/chromium expo start --dev-client --clear",
    "start:dev:atlas": "EXPO_ATLAS=true APP_VARIANT=development EDGE_PATH=/etc/profiles/per-user/stef/bin/chromium expo start --dev-client",
    "prebuild": "EXPO_NO_GIT_STATUS=1 expo prebuild --no-install --clean --platform android",
    "prebuild:dev": "EXPO_NO_GIT_STATUS=1 APP_VARIANT=development expo prebuild --no-install --clean --platform android",
    "prebuild:dev:atlas": "EXPO_ATLAS=true EXPO_NO_GIT_STATUS=1 APP_VARIANT=development expo prebuild --no-install --clean --platform android",
    "run:android": "expo run:android",
    "materialSymbols:setup": "bun scripts/setupMaterialSymbols.ts",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
